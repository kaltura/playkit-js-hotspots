{
  "name": "@plugin/core",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "type-check": "tsc --noEmit",
    "serve": "npm run build:copy-files && (npm run build:types -- --watch & npm run build:js -- --watch)",
    "build": "npm run build:types && npm run build:js && npm run build:copy-files",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps",
    "build:copy-files": "node ./scripts/copy-files.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "fs-extra": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "preact": "^8.4.2"
  }
}
